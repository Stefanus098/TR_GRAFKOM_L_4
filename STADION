void tribun_set_kiri(){
    //JEDA TRIBUN KIRI
    //JEDA TRIBUN MIRING KIRI
    glBegin(GL_QUADS);
    glColor3f(112/255.0f, 128/255.0f, 145/255.0f);
    glVertex3f(-50.0, 0.05, -38.0);
    glVertex3f(-55.0, 5.0, -38.0);
    glVertex3f(-55.0, 5.0, 38.0);
    glVertex3f(-50.0, 0.05, 38.0);
    glEnd();

    //JEDA TRIBUN DATAR KIRI
    glBegin(GL_QUADS);
    glColor3f(119/255.0f, 136/255.0f, 153/255.0f);
    glVertex3f(-55.0, 5, -38.0);
    glVertex3f(-60.0, 5, -38.0);
