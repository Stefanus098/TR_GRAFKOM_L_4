    glColor3f(1.0, 1.0, 1.0);
    glVertex3f(40.0, 0.05, 35.0);
    glVertex3f(37.0, 0.05, 35.0);
    glVertex3f(40.0, 0.05, 32.0);
    glEnd();

    //GARIS SUDUT LAPANGAN BELAKANG KIRI
    glBegin(GL_LINE_LOOP);
    glColor3f(1.0, 1.0, 1.0);
    glVertex3f(-40.0, 0.05, -35.0);
    glVertex3f(-37.0, 0.05, -35.0);
    glVertex3f(-40.0, 0.05, -32.0);
    glEnd();

    //GARIS SUDUT LAPANGAN BELAKANG KANAN
    glBegin(GL_LINE_LOOP);
    glColor3f(1.0, 1.0, 1.0);
    glVertex3f(40.0, 0.05, -35.0);
    glVertex3f(38.0, 0.05, -35.0);
    glVertex3f(40.0, 0.05, -32.0);
    glEnd();
}

