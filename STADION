void tiang_corner(){
    //TIANG CORNER DEPAN KIRI
    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.0, 0.05, 35.0);
    glVertex3f(-40.0, 0.05, 35.2);
    glVertex3f(-40.0, 10.0, 35.2);
    glVertex3f(-40.0, 10.0, 35.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.0, 0.05, 35.2);
    glVertex3f(-40.2, 0.05, 35.2);
    glVertex3f(-40.2, 10.0, 35.2);
    glVertex3f(-40.0, 10.0, 35.2);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.2, 0.05, 35.0);
    glVertex3f(-40.2, 0.05, 35.2);
    glVertex3f(-40.2, 10.0, 35.2);
    glVertex3f(-40.2, 10.0, 35.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.0, 0.05, 35.0);
    glVertex3f(-40.2, 0.05, 35.0);
    glVertex3f(-40.2, 10.0, 35.0);
    glVertex3f(-40.0, 10.0, 35.0);
    glEnd();

    //TIANG CORNER BELAKANG KIRI
    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.0, 0.05, -35.0);
    glVertex3f(-40.0, 0.05, -35.2);
    glVertex3f(-40.0, 10.0, -35.2);
    glVertex3f(-40.0, 10.0, -35.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.0, 0.05, -35.2);
    glVertex3f(-40.2, 0.05, -35.2);
    glVertex3f(-40.2, 10.0, -35.2);
    glVertex3f(-40.0, 10.0, -35.2);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.2, 0.05, -35.0);
    glVertex3f(-40.2, 0.05, -35.2);
    glVertex3f(-40.2, 10.0, -35.2);
    glVertex3f(-40.2, 10.0, -35.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(255/255.0f, 255/255.0f, 0.0);
    glVertex3f(-40.0, 0.05, -35.0);
