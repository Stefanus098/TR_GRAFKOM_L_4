    glVertex3f(35.0, 0.05, 50.0);
    glVertex3f(35.0, 5.0, 50.0);
    glVertex3f(40.0, 5.0, 48.0);
    glVertex3f(40.0, 0.05, 48.0);
    glEnd();

    glBegin(GL_QUADS);//POJOK II
    glColor3f(119/255.0f, 136/255.0f, 153/255.0f);
    glVertex3f(40.0, 5.0, 47.0);
