    glVertex3f(-50.0, 0.05, 38.0);
    glVertex3f(-55.0, 5.0, 38.0);
    glVertex3f(-49.0, 5.0, 47.0);
    glEnd();


    //JEDA TRIBUN KIRI POJOK DEPAN
    //JEDA DATAR POJOK 1
    glBegin(GL_QUADS);
    glColor3f(119/255.0f, 136/255.0f, 153/255.0f);
