}

void tribun_set_pojok_kanan_depan(){
        //JEDA TRIBUN KANAN POJOK +X
    //JEDA POJOK MIRING 1
    glBegin(GL_QUADS);
    glColor3f(112/255.0f, 128/255.0f, 145/255.0f);
    glVertex3f(35.0, 0.05, 50.0);
    glVertex3f(40.0, 0.05, 47.0);
    glVertex3f(41.0, 5.0, 53.0);
    glVertex3f(35.0, 5.0, 55.0);
    glEnd();
    //JEDA POJOK MIRING 2
    glBegin(GL_QUADS);
    glColor3f(112/255.0f, 128/255.0f, 145/255.0f);
    glVertex3f(40.0, 0.05, 47.0);
    glVertex3f(45.0, 0.05, 43.0);
    glVertex3f(49.0, 5.0, 47.0);
    glVertex3f(41.0, 5.0, 53.0);
    glEnd();
    //JEDA POJOK MIRING 3
    glBegin(GL_QUADS);
    glColor3f(112/255.0f, 128/255.0f, 145/255.0f);
    glVertex3f(45.0, 0.05, 43.0);
