    glVertex3f(-90.0, 25, 56.0);
    glVertex3f(-90.0, 30, 56.0);
    glVertex3f(-90.0, 30, -56.0);
    glEnd();


    //TEMBOK
    //TEMBOK KIRI
    glBegin(GL_QUADS);
    glColor3f(211/255.0f, 211/255.0f, 211/255.0f);
    glVertex3f(-90.0, 25, -56.0);
    glVertex3f(-90.0, 25, 56.0);
    glVertex3f(-95, 0.5, 56.0);
    glVertex3f(-95, 0.5, -56.0);
    glEnd();

    //KUBUS
    glBegin(GL_QUADS);
    glColor3f(1,1,1);
    glVertex3f(-95.1, 0.5,  46.0);
    glVertex3f(-105.0, 0.5, 46.0);
    glVertex3f(-105.0, 0.5, -46.0);
    glVertex3f(-95.1, 0.5, -46.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(1,1,1);
    glVertex3f(-105.0, 0.5, 46.0);
    glVertex3f(-105.0, 15.5, 46.0);
    glVertex3f(-105.0, 15.5, -46.0);
    glVertex3f(-105.0, 0.5, -46.0);
    glEnd();

    glBegin(GL_QUADS);
